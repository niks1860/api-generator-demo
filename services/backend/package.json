{
  "name": "backend",
  "version": "0.0.1",
  "description": "Backend service providing API endpoints to manage user defined models and API configurations",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist *.tsbuildinfo .eslintcache",
    "pretest": "pnpm run rebuild",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "pnpm run rebuild",
    "start": "node -r source-map-support/register .",
    "prepare": "pnpm run build",
    "rebuild": "pnpm run clean && pnpm run build"
  },
  "repository": {
    "type": "git"
  },
  "author": "Nikunj Dhorajiya <niks1860@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^5.0.1",
    "@loopback/core": "^4.0.1",
    "@loopback/repository": "^5.0.1",
    "@loopback/rest": "^12.0.1",
    "@loopback/rest-explorer": "^5.0.1",
    "loopback-connector-mongodb": "^5.6.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/testlab": "^5.0.1",
    "@types/jest": "^28.1.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.21",
    "jest": "^28.1.1",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "typescript": "~4.7.3"
  }
}
